FRAMEWORK_SEARCH_PATHS = $(inherited) "$(SDKROOT)/Developer/Library/Frameworks" "$(PLATFORM_DIR)/Developer/Library/Frameworks" $(inherited) $(DEVELOPER_FRAMEWORKS_DIR) "$(PLATFORM_DIR)/Developer/Library/Frameworks" "$(DEVELOPER_DIR)/Platforms/iPhoneSimulator.platform/Developer/Library/Frameworks" "$(SDKROOT)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Expecta" "${PODS_ROOT}/Headers/Public/KIF" "${PODS_ROOT}/Headers/Public/Specta" "${PODS_ROOT}/Headers/Public/Swizzlean"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Expecta" -isystem "${PODS_ROOT}/Headers/Public/KIF" -isystem "${PODS_ROOT}/Headers/Public/Specta" -isystem "${PODS_ROOT}/Headers/Public/Swizzlean" -DKIF_XCTEST
OTHER_LDFLAGS = -ObjC -l"Pods-OOPCardsSimpleViewsTests-Expecta" -l"Pods-OOPCardsSimpleViewsTests-KIF" -l"Pods-OOPCardsSimpleViewsTests-Specta" -l"Pods-OOPCardsSimpleViewsTests-Swizzlean" -framework "CoreGraphics" -framework "Foundation" -framework "XCTest"
PODS_ROOT = ${SRCROOT}/Pods